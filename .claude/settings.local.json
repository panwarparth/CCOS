{
  "permissions": {
    "allow": [
      "Bash(dir)",
      "Bash(npm install)",
      "Bash(npm run db:generate:*)",
      "Bash(npm run db:push:*)",
      "Bash(Select-Object Name,Status)",
      "Bash(powershell -Command \"Get-Service -Name ''postgresql*'' 2>$null | Select-Object Name,Status; where.exe psql 2>$null\")",
      "Bash(powershell:*)",
      "Bash(npm run db:seed:*)",
      "Bash(npm run dev:*)",
      "Bash(npx prisma db push:*)",
      "Bash(npx prisma:*)",
      "Bash(npx tsc:*)",
      "Bash(git init:*)",
      "Bash(git add:*)"
    ]
  }
}
